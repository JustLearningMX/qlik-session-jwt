    "KeyID": "<YOUR_KEY_ID>",
    "TenantUrl": "<YOUR_QLIK_TENANT_URL>",
    "WebIntegrationID": "<YOUR_WEB_INTEGRATION_ID>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
